{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Webpack Stats",
  "description": "Statistics about modules generated by webpack",
  "type": "object",
  "properties": {
    "version": {
      "description": "Version of webpack used for the compilation",
      "type": "string"
    },
    "hash": {
      "description": "Compilation specific hash",
      "type": "string"
    },
    "time": {
      "description": "Compilation time in milliseconds",
      "type": "number"
    },
    "publicPath": {
      "type": "string"
    },
    "outputPath": {
      "description": "path to webpack output directory",
      "type": "string"
    },
    "assetsByChunkName": {
      "description": "Chunk name to emitted asset(s) mapping",
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "assets": {
      "description": "A list of asset objects",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "chunkNames": {
            "description": "The chunks this asset contains",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "chunks": {
            "description": "The chunk IDs this asset contains",
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "comparedForEmit": {
            "description": "Indicates whether or not the asset was compared with the same file on the output file system",
            "type": "string"
          },
          "emitted": {
            "description": "Indicates whether or not the asset made it to the `output` directory",
            "type": "boolean"
          },
          "name": {
            "description": "The `output` filename",
            "type": "string"
          },
          "size": {
            "description": "The size of the file in bytes",
            "type": "number"
          },
          "info": {
            "type": "object",
            "properties": {
              "immutable": {
                "description": "A flag telling whether the asset can be long term cached (contains a hash)",
                "type": "boolean"
              },
              "size": {
                "description": "The size in bytes, only becomes available after asset has been emitted",
                "type": "number"
              },
              "development": {
                "description": "A flag telling whether the asset is only used for development and doesn't count towards user-facing assets",
                "type": "boolean"
              },
              "hotModuleReplacement": {
                "description": "A flag telling whether the asset ships data for updating an existing application (HMR)",
                "type": "boolean"
              },
              "sourceFilename": {
                "description": "sourceFilename when asset was created from a source file (potentially transformed)",
                "type": "string"
              },
              "javascriptModule": {
                "description": "true, when asset is javascript and an ESM",
                "type": "boolean"
              }
            },
            "required": [
              "immutable",
              "size",
              "development",
              "hotModuleReplacement",
              "sourceFilename",
              "javascriptModule"
            ],
            "additionalProperties": false
          }
        },
        "required": [
          "chunkNames",
          "chunks",
          "comparedForEmit",
          "emitted",
          "name",
          "size",
          "info"
        ],
        "additionalProperties": false
      }
    },
    "chunks": {
      "description": "A list of chunk objects",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "entry": {
            "type": "boolean",
            "description": "Indicates whether or not the chunk contains the webpack runtime"
          },
          "files": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "An array of filename strings that contain this chunk"
          },
          "filteredModules": {
            "type": "number",
            "description": "See the description in the top-level structure above"
          },
          "id": {
            "type": "number",
            "description": "The ID of this chunk"
          },
          "initial": {
            "type": "boolean",
            "description": "Indicates whether this chunk is loaded on initial page load or on demand"
          },
          "modules": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "A list of module objects"
          },
          "names": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "An list of chunk names contained within this chunk"
          },
          "origins": {
            "type": "array",
            "description": "list of origins describing how the given chunk originated.",
            "items": {
              "type": "object",
              "properties": {
                "loc": {
                  "type": "string",
                  "description": "Lines of code that generated this chunk"
                },
                "module": {
                  "type": "string",
                  "description": "Path to the module"
                },
                "moduleId": {
                  "type": "number",
                  "description": "The ID of the module"
                },
                "moduleIdentifier": {
                  "type": "string",
                  "description": "Path to the module"
                },
                "moduleName": {
                  "type": "string",
                  "description": "Relative path to the module"
                },
                "name": {
                  "type": "string",
                  "description": "The name of the chunk"
                },
                "reasons": {
                  "type": "array",
                  "description": "A list of the same `reasons` found in module objects",
                  "items": {
                    "$ref": "#/$defs/reason"
                  }
                }
              },
              "required": [
                "loc",
                "module",
                "moduleId",
                "moduleIdentifier",
                "moduleName",
                "name",
                "reasons"
              ],
              "additionalProperties": false
            }
          },
          "parents": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "Parent chunk IDs"
          },
          "rendered": {
            "type": "boolean",
            "description": "Indicates whether or not the chunk went through Code Generation"
          },
          "size": {
            "type": "number",
            "description": "Chunk size in bytes"
          }
        },
        "required": [
          "entry",
          "files",
          "filteredModules",
          "id",
          "initial",
          "modules",
          "names",
          "origins",
          "parents",
          "rendered",
          "size"
        ],
        "additionalProperties": false
      }
    },
    "modules": {
      "description": "A list of module objects",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "assets": {
            "type": "array",
            "description": "A list of asset objects"
          },
          "built": {
            "type": "boolean",
            "description": "Indicates that the module went through Loaders, Parsing, and Code Generation"
          },
          "cacheable": {
            "type": "boolean",
            "description": "Whether or not this module is cacheable"
          },
          "chunks": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "IDs of chunks that contain this module"
          },
          "errors": {
            "type": "number",
            "description": "Number of errors when resolving or processing the module"
          },
          "failed": {
            "type": "boolean",
            "description": "Whether or not compilation failed on this module"
          },
          "id": {
            "type": "number",
            "description": "The ID of the module"
          },
          "identifier": {
            "type": "string",
            "description": "A unique ID used internally"
          },
          "name": {
            "type": "string",
            "description": "Path to the actual file"
          },
          "optional": {
            "type": "boolean",
            "description": "All requests to this module are with `try... catch` blocks"
          },
          "prefetched": {
            "type": "boolean",
            "description": "Indicates whether or not the module was prefetched"
          },
          "profile": {
            "type": "object",
            "properties": {
              "building": {
                "type": "number",
                "description": "Loading and parsing"
              },
              "dependencies": {
                "type": "number",
                "description": "Building dependencies"
              },
              "factory": {
                "type": "number",
                "description": "Resolving dependencies"
              }
            },
            "required": ["building", "dependencies", "factory"],
            "additionalProperties": false,
            "description": "Module specific compilation stats corresponding to the --profile flag (in milliseconds)"
          },
          "reasons": {
            "type": "array",
            "items": {
              "$ref": "#/$defs/reason"
            }
          },
          "size": {
            "type": "number",
            "description": "Estimated size of the module in bytes"
          },
          "source": {
            "type": "string",
            "description": "The stringified raw source"
          },
          "warnings": {
            "type": "number",
            "description": "Number of warnings when resolving or processing the module"
          }
        },
        "required": [
          "assets",
          "built",
          "cacheable",
          "chunks",
          "errors",
          "failed",
          "id",
          "identifier",
          "name",
          "optional",
          "prefetched",
          "reasons",
          "size",
          "source",
          "warnings"
        ],
        "additionalProperties": false
      }
    },
    "entryPoints": {
      "description": "A list of entry objects",
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "chunks": {
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "assets": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["name", "size"],
              "properties": {
                "name": {
                  "type": "string"
                },
                "size": {
                  "type": "number"
                }
              }
            }
          },
          "filteredAssets": {
            "type": "string"
          },
          "assetsSize": {
            "type": "number"
          },
          "auxiliaryAssets": {
            "type": "array"
          },
          "filteredAuxiliaryAssets": {
            "type": "string"
          },
          "auxiliaryAssetsSize": {
            "type": "string"
          },
          "children": {
            "type": "object"
          },
          "childAssets": {
            "type": "object"
          },
          "isOverSizeLimit": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "chunks",
          "assets",
          "filteredAssets",
          "assetsSize",
          "auxiliaryAssets",
          "filteredAuxiliaryAssets",
          "auxiliaryAssetsSize",
          "children",
          "childAssets",
          "isOverSizeLimit"
        ],
        "additionalProperties": false
      }
    },
    "errors": {
      "description": "A list of error objects",
      "type": "array",
      "items": {
        "$ref": "#/$defs/error"
      }
    },
    "errorsCount": {
      "description": "number of errors",
      "type": "number"
    },
    "warnings": {
      "description": "A list of warning objects",
      "type": "array",
      "items": {
        "$ref": "#/$defs/error"
      }
    },
    "warningsCount": {
      "description": "number of warnings",
      "type": "number"
    }
  },
  "required": [
    "version",
    "hash",
    "time",
    "publicPath",
    "outputPath",
    "assetsByChunkName",
    "assets",
    "chunks",
    "modules",
    "entryPoints",
    "errors",
    "errorsCount",
    "warnings",
    "warningsCount"
  ],
  "additionalProperties": false,

  "$defs": {
    "reason": {
      "description": "describes why a module was included in the dependency graph.",
      "type": "object",
      "properties": {
        "loc": {
          "type": "string",
          "description": "Lines of code that caused the module to be included"
        },
        "module": {
          "type": "string",
          "description": "Relative path to the module based on context"
        },
        "moduleId": {
          "type": "number",
          "description": "The ID of the module"
        },
        "moduleIdentifier": {
          "type": "string",
          "description": "Path to the module"
        },
        "moduleName": {
          "type": "string",
          "description": "A more readable name for the module (used for 'pretty-printing')"
        },
        "type": {
          "type": "string",
          "description": "The type of request used"
        },
        "userRequest": {
          "type": "string",
          "description": "Raw string used for the 'import' or 'require' request"
        }
      },
      "required": [
        "loc",
        "module",
        "moduleId",
        "moduleIdentifier",
        "moduleName",
        "type",
        "userRequest"
      ],
      "additionalProperties": false
    },
    "error": {
      "type": "object",
      "properties": {
        "moduleIdentifier": {
          "type": "string"
        },
        "moduleName": {
          "type": "string"
        },
        "loc": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "moduleId": {
          "type": "number"
        },
        "moduleTrace": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "originIdentifier": {
                "type": "string"
              },
              "originName": {
                "type": "string"
              },
              "moduleIdentifier": {
                "type": "string"
              },
              "moduleName": {
                "type": "string"
              },
              "dependencies": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": ["loc"],
                  "properties": {
                    "loc": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": false
                }
              },
              "originId": {
                "type": "number"
              },
              "moduleId": {
                "type": "number"
              }
            },
            "required": [
              "originIdentifier",
              "originName",
              "moduleIdentifier",
              "moduleName",
              "dependencies",
              "originId",
              "moduleId"
            ],
            "additionalProperties": false
          }
        },
        "details": {
          "type": "string"
        },
        "stack": {
          "type": "string"
        }
      },
      "required": [
        "moduleIdentifier",
        "moduleName",
        "loc",
        "message",
        "moduleId",
        "moduleTrace",
        "details"
      ],
      "additionalProperties": false
    }
  }
}
